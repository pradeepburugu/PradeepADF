{
	"name": "pl_read_from_local_storage_self_ir",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "FileServerReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_pradeep_local_file_system",
						"type": "DatasetReference",
						"parameters": {
							"sheetName": {
								"value": "@pipeline().parameters.sheetName",
								"type": "Expression"
							},
							"folderPath": {
								"value": "@pipeline().parameters.folderPath",
								"type": "Expression"
							},
							"fileName": {
								"value": "@pipeline().parameters.fileName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_test_gen2_biiling_report",
						"type": "DatasetReference",
						"parameters": {
							"source_folder": "test/source",
							"Source_File_Name": {
								"value": "@concat(pipeline().parameters.sheetName,'.csv')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"sheetName": {
				"type": "string",
				"defaultValue": "Payer Unit Prices"
			},
			"fileName": {
				"type": "string",
				"defaultValue": "Meal Price for GBC - CITY FRESH - SCES.xlsx"
			},
			"folderPath": {
				"type": "string",
				"defaultValue": "COC\\SCES"
			}
		},
		"folder": {
			"name": "Learning"
		},
		"annotations": []
	}
}